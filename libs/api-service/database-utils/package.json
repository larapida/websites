{
  "name": "@larapida-websites/api-service-database-utils",
  "version": "0.0.1",
  "private": true,
  "main": "./dist/libs/api-service/database-utils/index.js",
  "module": "./dist/libs/api-service/database-utils/index.js",
  "types": "./dist/libs/api-service/database-utils/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/libs/api-service/database-utils/index.d.ts",
      "import": "./dist/libs/api-service/database-utils/index.js",
      "default": "./dist/libs/api-service/database-utils/index.js"
    }
  },
  "dependencies": {
    "@swc/helpers": "~0.5.17"
  },
  "devDependencies": {},
  "nx": {
    "projectType": "library",
    "tags": [
      "scope:api",
      "type:lib"
    ],
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "dist/libs/api-service/database-utils",
          "tsConfig": "libs/api-service/database-utils/tsconfig.lib.json",
          "packageJson": "libs/api-service/database-utils/package.json",
          "main": "libs/api-service/database-utils/src/index.ts",
          "stripLeadingPaths": true,
          "clean": true,
          "assets": [
            {
              "input": "libs/api-service/database-utils/src/prisma",
              "glob": "*.node",
              "output": "prisma"
            }
          ]
        },
        "dependsOn": [
          {
            "projects": "@larapida-websites/prisma",
            "target": "generate",
            "params": "forward"
          }
        ],
        "configurations": {
          "development": {
            "dependsOn": [
              {
                "projects": "@larapida-websites/prisma:development",
                "target": "generate",
                "params": "forward"
              }
            ]
          },
          "production": {
            "dependsOn": [
              {
                "projects": "@larapida-websites/prisma:production",
                "target": "generate",
                "params": "forward"
              }
            ]
          }
        }
      }
    }
  }
}
