{
  "name": "@larapida-websites/www-env",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/www/env/src",
  "projectType": "library",
  "tags": ["type:lib", "scope:www"],
  "// targets": "to see all targets run: nx show project @larapida-websites/www-env --web",
  "targets": {
    "inject": {
      "executor": "@larapida-websites/dotenv-manager:deploy",
      "options": {
        "scope": "www"
      },
      "configurations": {
        "development": {
          "options": {
            "environment": "development"
          }
        },
        "production": {
          "options": {
            "environment": "production"
          }
        }
      },
      "defaultConfiguration": "development"
    }
  }
}
