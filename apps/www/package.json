{
  "name": "@larapida-websites/www",
  "private": true,
  "type": "module",
  "scripts": {},
  "engines": {
    "node": ">=20"
  },
  "sideEffects": false,
  "dependencies": {
    "@react-router/node": "^7.2.0",
    "@react-router/serve": "^7.2.0",
    "isbot": "^4.4.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-router": "^7.2.0"
  },
  "devDependencies": {
    "@react-router/dev": "^7.2.0",
    "@types/node": "^20.0.0",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0"
  },
  "nx": {
    "projectType": "application",
    "tags": [
      "type:app",
      "scope:www",
      "platform:client"
    ],
    "targets": {
      "typegen": {
        "executor": "nx:run-commands",
        "options": {
          "cwd": "apps/www",
          "commands": [
            "react-router typegen"
          ]
        },
        "outputs": [
          "{projectRoot}/.react-router"
        ]
      },
      "typecheck": {
        "dependsOn": [
          "typegen",
          "^typecheck"
        ]
      },
      "build": {
        "dependsOn": [
          {
            "projects": [
              "@larapida-websites/www-ecosystem"
            ],
            "target": "pre-inject-env",
            "params": "forward"
          },
          "typecheck",
          "^build"
        ],
        "parallel": false,
        "configurations": {
          "development": {
            "dependsOn": [
              {
                "projects": [
                  "@larapida-websites/www-ecosystem"
                ],
                "target": "pre-inject-env:development",
                "params": "forward"
              },
              "typecheck",
              "^build"
            ],
            "parallel": false
          },
          "production": {
            "dependsOn": [
              {
                "projects": [
                  "@larapida-websites/www-ecosystem"
                ],
                "target": "pre-inject-env:production",
                "params": "forward"
              },
              "typecheck",
              "^build"
            ],
            "parallel": false
          }
        }
      }
    }
  }
}
