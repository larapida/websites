{
  "name": "@larapida-websites/cli",
  "version": "0.0.1",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "dependencies": {
    "@swc/helpers": "~0.5.11",
    "commander": "^13.1.0",
    "inquirer": "^12.6.1",
    "bcryptjs": "^3.0.2"
  },
  "devDependencies": {},
  "nx": {
    "sourceRoot": "tools/cli/src",
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "tools/cli/dist",
          "main": "tools/cli/src/index.ts",
          "tsConfig": "tools/cli/tsconfig.lib.json",
          "skipTypeCheck": true,
          "stripLeadingPaths": true
        },
        "dependsOn": [
          {
            "projects": [
              "@larapida-websites/prisma-tools"
            ],
            "target": "generate",
            "params": "forward"
          },
          "^build"
        ],
        "parallel": false,
        "defaultConfiguration": "development",
        "configurations": {
          "development": {
            "dependsOn": [
              {
                "projects": [
                  "@larapida-websites/prisma-tools"
                ],
                "target": "generate:development",
                "params": "forward"
              },
              "^build"
            ],
            "parallel": false
          },
          "production": {
            "development": {
              "dependsOn": [
                {
                  "projects": [
                    "@larapida-websites/prisma-tools"
                  ],
                  "target": "generate:production",
                  "params": "forward"
                },
                "^build"
              ],
              "parallel": false
            }
          }
        }
      }
    }
  }
}
