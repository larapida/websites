{
  "name": "@larapida-websites/dotenv-manager",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "tools/dotenv-manager/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:swc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "{workspaceRoot}/dist/tools/dotenv-manager",
        "main": "tools/dotenv-manager/src/index.ts",
        "tsConfig": "tools/dotenv-manager/tsconfig.lib.json",
        "skipTypeCheck": true,
        "stripLeadingPaths": true,
        "assets": [
          {
            "input": "./tools/dotenv-manager/src",
            "glob": "**/!(*.ts)",
            "output": "."
          },
          {
            "input": "./tools/dotenv-manager/src",
            "glob": "**/*.d.ts",
            "output": "."
          }
        ]
      }
    }
  }
}
