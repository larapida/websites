{
  "name": "@larapida-websites/ecosystem",
  "version": "0.0.1",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "nx": {
    "sourceRoot": "tools/ecosystem/src",
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "tools/ecosystem/dist",
          "main": "tools/ecosystem/src/index.ts",
          "tsConfig": "tools/ecosystem/tsconfig.lib.json",
          "skipTypeCheck": true,
          "stripLeadingPaths": true,
          "assets": [
            {
              "input": "./tools/ecosystem/src",
              "glob": "**/!(*.ts)",
              "output": "."
            },
            {
              "input": "./tools/ecosystem/src",
              "glob": "**/*.d.ts",
              "output": "."
            }
          ]
        }
      }
    }
  },
  "dependencies": {
    "@nx/devkit": "20.8.1",
    "@swc/helpers": "~0.5.11"
  },
  "executors": "./executors.json",
  "files": [
    "dist",
    "!**/*.tsbuildinfo",
    "executors.json"
  ]
}
